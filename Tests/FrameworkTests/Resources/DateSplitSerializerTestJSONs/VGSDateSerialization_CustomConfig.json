         "monthFieldName":"month",
         "dayFieldName":"day",
         "yearFieldName":"year",
         "fieldValue":"2030-/-01-/-15",
         "expectedResult":{
            "month":"01",
            "day":"15",
            "year":"2030"
         },
         "comment":"Single key fieldNames."
      },
      {
         "monthFieldName":"date.month",
         "dayFieldName":"date.day",
